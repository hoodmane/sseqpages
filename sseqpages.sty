\def\sseq@class@@(#1,#2)#3#4{%
    \sseq@tempx=#1\relax
    \advance\sseq@tempx\sseq@x\relax
    \sseq@tempy=#2\relax
    \advance\sseq@tempy\sseq@y\relax
    \edef\partialcoord{\the\sseq@tempx,\the\sseq@tempy}%
    \@xp\sseq@xminmax\@xp{\the\sseq@tempx}\@xp\sseq@yminmax\@xp{\the\sseq@tempy}% TODO: Don't expand range to accomodate nodes that are out of range in one direction
    \sseq@tempcount=\@ifundefined{sseq@coord\sseq@thename(\partialcoord)@numnodespp}{1}{\csname sseq@coord\sseq@thename(\partialcoord)@numnodespp\endcsname}%
    \edef\nodenum{\the\sseq@tempcount}%
    \ifsseq@hasname
        \ifnum\sseq@tempcount=1\relax
            \@xp\def\csname sseq@coord\sseq@thename(\partialcoord)@numnodes\endcsname{0}
            \@xp\def\csname sseq@coord\sseq@thename(\partialcoord)@numnodespp\endcsname{1}
        \fi
        \@ifundefined{sseq@coord@changed(\partialcoord)}{
            \@xp\gdef\csname sseq@coord@changed(\partialcoord)\endcsname{}
            \ifsseq@keepchanges\else
                \sseq@x@addto@macro@noctx\sseq@pagecleanup{
                    \gdef\@xp\@nx\csname sseq@coord\sseq@thename(\partialcoord)@numnodes\endcsname{\csname sseq@coord\sseq@thename(\partialcoord)@numnodes\endcsname}
                    \gdef\@xp\@nx\csname sseq@coord\sseq@thename(\partialcoord)@numnodespp\endcsname{\csname sseq@coord\sseq@thename(\partialcoord)@numnodespp\endcsname}
                    \@nx\sseq@setoffsets{\the\sseq@tempx}{\the\sseq@tempy}
                }
            \fi
            \sseq@x@addto@macro\sseq@compilesseq{%
                \@nx\sseq@setoffsets{\the\sseq@tempx}{\the\sseq@tempy}%
                \global\let\@xp\@nx\csname sseq@coord@changed(\partialcoord)\endcsname\relax
            }%
        }{}
    \fi

    \@xp\xdef\csname sseq@coord\sseq@thename(\partialcoord)@numnodes\endcsname{\the\sseq@tempcount}%
    \advance\sseq@tempcount1\relax
    \@xp\xdef\csname sseq@coord\sseq@thename(\partialcoord)@numnodespp\endcsname{\the\sseq@tempcount}
    \edef\coord{(\the\sseq@tempx,\the\sseq@tempy,\nodenum)}%
%
    \sseq@restorestyles
    \sseq@temptoks\@xp{\@xp\def\@xp\sseq@classnodetext\@xp{\sseq@classnodetext}}
    \sseq@temptoks@ii\@xp{\@xp\def\@xp\sseq@classnodetextoptions\@xp{\sseq@classnodetextoptions}}
    \edef\temp{\sseq@temptoks{\the\sseq@temptoks\the\sseq@temptoks@ii}}\temp
    \sseq@temptoks@ii\@xp{\@xp\def\@xp\sseq@classlabelnodes\@xp{\sseq@classlabelnodes}}
    \edef\temp{\sseq@temptoks{\the\sseq@temptoks\the\sseq@temptoks@ii}}\temp
%
    \sseq@temptoks@ii{\pgfkeysalso{#4}}% local options
%
    \@xp\gdef\csname sseq@classnum\sseq@thename\coord\endcsname{0}% For \classreplace
    \ifsseq@keepchanges
        % Classes that eventually die
        % we use deforaddto@macro here because this macro gets moved, so any context needs to be installed as part of the sseq@class command
        \sseq@x@deforaddto@macro{sseq@class\sseq@thename\coord0}{%
            \begingroup
            \ifsseq@needstikz \@nx\sseq@needstikztrue\fi
            \@nx\sseq@class@drawnode{#1}{\the\sseq@x}{#2}{\the\sseq@y}{\nodenum}{\the\sseq@temptoks}{%
                \sseq@thepathstyle\sseq@thenodestyle\sseq@theclassstyle\sseq@thetransientcyclestyle
                \@nx\ifsseq@thispage\sseq@thethispagecyclestyle\@nx\fi
                \the\sseq@scopeoptions
                \the\sseq@temptoks@ii
                \@xp\@nx\csname sseq@classstyle\sseq@thename\coord\endcsname
            }%
            \endgroup
        }%
    %
        % Classes that live forever
        \@xp\xdef\csname sseq@classinfty\sseq@thename\coord0\endcsname{%
            \begingroup
            \ifsseq@needstikz \@nx\sseq@needstikztrue \edef\@nx\tikz@shape{\tikz@shape}\fi
            \@nx\sseq@class@drawnode{#1}{\the\sseq@x}{#2}{\the\sseq@y}{\nodenum}{\the\sseq@temptoks}{%
                \sseq@thepathstyle\sseq@thenodestyle\sseq@theclassstyle\sseq@thepermanentcyclestyle
                \the\sseq@scopeoptions
                \the\sseq@temptoks@ii
                \@xp\@nx\csname sseq@classstyle\sseq@thename\coord\endcsname
            }%
            \endgroup
        }%
        \sseq@x@deforaddto@macro@noctx{sseq@clearinfty\sseq@thename\coord}{%
            \global\let\@xp\@nx\csname sseq@classinfty\sseq@thename\coord0\endcsname\relax
        }%
        \sseq@temptoks\@xp\@xp\@xp{\csname sseq@classinfty\sseq@thename\coord0\endcsname}
        \sseq@x@deforaddto@macro@noctx{sseq@clearinftytemporary\sseq@thename\coord}{%
            \@nx\sseq@g@addto@macro@noctx\@nx\sseq@pagecleanup{\gdef\@xp\@nx\csname sseq@classinfty\sseq@thename\coord0\endcsname{\the\sseq@temptoks}}
            \global\let\@xp\@nx\csname sseq@classinfty\sseq@thename\coord0\endcsname\relax
        }%
        \@xp\sseq@x@addto@macro\csname sseq@savednodes@\sseq@thename infty\endcsname{% no need for defor since infty page initialized at begin{sseqdata}%
            \@xp\@nx\csname sseq@classinfty\sseq@thename\coord0\endcsname %
        }%
    \else
        \@xp\xdef\csname sseq@permanentcyclestyle\sseq@thename\coord0\endcsname{\the\sseq@permanentcyclestyle}
        \@xp\xdef\csname sseq@transientcyclestyle\sseq@thename\coord0\endcsname{\the\sseq@transientcyclestyle}
        \sseq@x@addto@macro\sseq@thispagenodes{%
            \begingroup
            \@xp\@nx\csname sseq@classisthispage\sseq@thename\coord\endcsname
            \ifsseq@needstikz \@nx\sseq@needstikztrue \edef\@nx\tikz@shape{\tikz@shape}\fi
            \@nx\sseq@class@drawnode{#1}{\the\sseq@x}{#2}{\the\sseq@y}{\nodenum}{\the\sseq@temptoks}{%
                \the\sseq@pathstyle\the\sseq@nodestyle\the\sseq@classstyle
                \@nx\csname sseq@\@xp\@nx\csname sseq@classpermortrans\sseq@thename\coord0\endcsname cyclestyle\sseq@thename\coord0\endcsname
                \@nx\ifsseq@thispage\sseq@thethispagecyclestyle\@nx\fi
                \the\sseq@scopeoptions
                \the\sseq@temptoks@ii
                \@xp\@nx\csname sseq@classstyle\sseq@thename\coord\endcsname
            }%
            \endgroup
        }%
        \@xp\gdef\csname sseq@class\sseq@thename\coord0\endcsname{}
        \@xp\gdef\csname sseq@classisthispage\sseq@thename\coord\endcsname{}
        \@xp\gdef\csname sseq@classpermortrans\sseq@thename\coord0\endcsname{permanent}
        \sseq@x@deforaddto@macro@noctx{sseq@clearinftytemporary\sseq@thename\coord}{\gdef\@xp\@nx\csname sseq@classpermortrans\sseq@thename\coord0\endcsname{transient}}
        \sseq@setoffsets{\the\sseq@tempx}{\the\sseq@tempy}
    \fi
    \@xp\gdef\csname sseq@classstyle\sseq@thename\coord\endcsname{}%
    \endgroup
}

