\def\sseq@replaceclass@@#1{%
    \sseq@arg@checkpresent\replaceclass1%
    \sseq@arg@checkpresent\replaceclass2%
    \sseq@tempx=\sseq@x
    \advance\sseq@tempx\sseq@arg1\relax
    \sseq@tempy=\sseq@y
    \advance\sseq@tempy\sseq@arg2\relax
    \sseq@whichclass\sseq@n{\the\sseq@tempx}{\the\sseq@tempy}{\sseq@optarg{}3}{replace class command '\sseq@thiscall'}
    \edef\coord{(\sseq@arg1,\sseq@arg2,\sseq@n)}%
    \edef\coordname{(\sseq@arg1,\sseq@arg2\sseq@optargc3)}%
    \sseq@ifundefcoord\coord{\sseq@error{There never was a class at \coordname\sseq@macroname\ so I cannot replace it}{}}{}%
    \sseq@ifdead\coord{}{\sseq@error{I cannot replace the class at \coordname\sseq@macroname\ because it seems to be alive and well}{}}%
    \sseq@tempcount=\csname sseq@classnum\sseq@thename\coord\endcsname\relax
    \ifsseq@keepchanges\else
        \sseq@x@addto@macro@noctx\sseq@pagecleanup{%
            \gdef\@xp\@nx\csname sseq@classnum\sseq@thename\coord\endcsname{\the\sseq@tempcount}%
        }%
    \fi
    \advance\sseq@tempcount1\relax
    \edef\classversion{\the\sseq@tempcount}%
    \@xp\xdef\csname sseq@classnum\sseq@thename\coord\endcsname{\the\sseq@tempcount}%
%
    \sseq@restorestyles
%
    \sseq@temptoks\@xp{\@xp\def\@xp\sseq@classnodetext\@xp{\sseq@classnodetext}}%
    \sseq@temptoks@ii\@xp{\@xp\def\@xp\sseq@classnodetextoptions\@xp{\sseq@classnodetextoptions}}%
    \edef\temp{\sseq@temptoks{\the\sseq@temptoks\the\sseq@temptoks@ii}}\temp
    \sseq@temptoks@ii\@xp{\@xp\def\@xp\sseq@classlabelnodes\@xp{\sseq@classlabelnodes}}%
    \edef\temp{\sseq@temptoks{\the\sseq@temptoks\the\sseq@temptoks@ii}}\temp
%
    \sseq@temptoks@ii{\pgfkeysalso{#1}}% local options
%
    \ifsseq@keepchanges
        % Classes that eventually die
        % we use deforaddto@macro here because this macro gets moved around, so any context needs to be installed as part of the sseq@class command
        \sseq@x@deforaddto@macro{sseq@class\sseq@thename\coord\classversion}{%
            \@nx\ifnum\@nx\sseq@thepage>\sseq@getdeathpage\coord\relax
            \begingroup
            \ifsseq@needstikz \@nx\sseq@needstikztrue\fi
            \@nx\sseq@class@drawnode{\sseq@arg1}{\the\sseq@x}{\sseq@arg2}{\the\sseq@y}{\sseq@n}{\the\sseq@temptoks}{%
                \sseq@thepathstyle\sseq@thenodestyle\sseq@theclassstyle
                \sseq@thetransientcyclestyle\@nx\ifsseq@thispage\sseq@thethispagecyclestyle\@nx\fi
                \the\sseq@scopeoptions
                \the\sseq@temptoks@ii
                \@xp\@nx\csname sseq@classstyle\sseq@thename\coord\endcsname
            }%
            \endgroup
            \@nx\fi
        }%
%
        % Classes that live forever
        \@xp\xdef\csname sseq@classinfty\sseq@thename\coord\classversion\endcsname{%
            \@nx\ifnum\@nx\sseq@thepage>\sseq@getdeathpage\coord\relax
            \begingroup
            \ifsseq@needstikz \@nx\sseq@needstikztrue \edef\@nx\tikz@shape{\tikz@shape}\fi
            \@nx\sseq@class@drawnode{\sseq@arg1}{\the\sseq@x}{\sseq@arg2}{\the\sseq@y}{\sseq@n}{\the\sseq@temptoks}{%
                \sseq@thepathstyle\sseq@thenodestyle\sseq@theclassstyle\sseq@thepermanentcyclestyle
                \the\sseq@scopeoptions
                \the\sseq@temptoks@ii
                \@xp\@nx\csname sseq@classstyle\sseq@thename\coord\endcsname
            }%
            \endgroup
            \@nx\fi
        }%
%
        \sseq@x@deforaddto@macro{sseq@clearinfty\sseq@thename\coord}{\global\let\@xp\@nx\csname sseq@classinfty\sseq@thename\coord\classversion\endcsname\relax}%
        \@xp\sseq@x@addto@macro\csname sseq@savednodes@\sseq@thename infty\endcsname{% no need for defor since infty page initialized at begin{sseqdata}%
            \@xp\@nx\csname sseq@classinfty\sseq@thename\coord\classversion\endcsname %
        }%
        \sseq@temptoks\@xp\@xp\@xp{\csname sseq@classinfty\sseq@thename\coord\classversion\endcsname}
        \sseq@x@deforaddto@macro@noctx{sseq@clearinftytemporary\sseq@thename\coord}{%
            \@nx\sseq@g@addto@macro@noctx\@nx\sseq@pagecleanup{\gdef\@xp\@nx\csname sseq@classinfty\sseq@thename\coord\classversion\endcsname{\the\sseq@temptoks}}
            \global\let\@xp\@nx\csname sseq@classinfty\sseq@thename\coord\classversion\endcsname\relax
        }%
        \sseq@cleardeathpage\coord
    \else
        \@xp\xdef\csname sseq@permanentcyclestyle\sseq@thename\coord\classversion\endcsname{\the\sseq@permanentcyclestyle}
        \@xp\xdef\csname sseq@transientcyclestyle\sseq@thename\coord\classversion\endcsname{\the\sseq@transientcyclestyle}
        \sseq@x@addto@macro\sseq@thispagenodes{%
            \begingroup
            \ifsseq@needstikz \@nx\sseq@needstikztrue \edef\@nx\tikz@shape{\tikz@shape}\fi
            \@nx\sseq@class@drawnode{\sseq@arg1}{\the\sseq@x}{\sseq@arg2}{\the\sseq@y}{\sseq@n}{\the\sseq@temptoks}{%
